{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalEnv": [
    "ENV",
    "OIDC_GOOGLE_CLIENT_ID",
    "OIDC_GOOGLE_CLIENT_SECRET",
    "API_URL"
  ]
}
